<!DOCTYPE html>
<html>
<%- include('partials/head.ejs') %>
<body class="w3-light-grey w3-content" style="max-width:1600px; background-repeat: no-repeat;background-attachment: fixed;background-size: cover; ">
<div class="se-pre-con"></div>
   <!-- Doctors List -->
<div class="transbox" style = "background-color: #ffffff;opacity: 0.6;filter: alpha(opacity=60);position:absolute; left:160px;top:55px; height:1000px; width:1100px "  id="dashb">
        <div class="wrapper wrapper--w680" style="max-width: 950px;">
            <div class="card card-4">
                <div class="card-body">
                   <h1 class="title" id="addreco" style="font-size:50px;font-weight: bold; color:#000000;font-family:'Lato',sans-serif;" >PRESCRIPTION LIST</h1>
                   <div class="search-container" style="float: center;
  padding: 6px 10px;
  margin-top: 8px;
  margin-right: 26px;
  background: #ffffff;
  font-size: 21px;
  border: none;
  cursor: pointer;">
   
  </div>
           <% for (var d=0;d<doc.length;d++){ %>
<div class="w3-container w3-card w3-white w3-round w3-margin" style="border-style: inset;border-color:blue;position:relative;box-shadow:0 12px 15px 0 rgba(0,0,0,.24),0 17px 50px 0 rgba(0,0,0,.19) ;"><br>

             <form method="post">
             <h4  style="font-size:30px;font-weight: bold; color:#0a0a0a;font-family:'Lato',sans-serif;">ASSET ID:<b style="font-size:25px;font-weight: bold; color:#264d73;font-family:'Lato',sans-serif;"> <%= doc[d].data.pid %></b>
             <h4  style="font-size:30px;font-weight: bold; color:#0a0a0a;font-family:'Lato',sans-serif;">EMAIL:<b style="font-size:25px;font-weight: bold; color:#264d73;font-family:'Lato',sans-serif;"> <%= arr[d] %></b>
             <h4  style="font-size:30px;font-weight: bold; color:#0a0a0a;font-family:'Lato',sans-serif;">ALLERGY:<b style="font-size:25px;font-weight: bold; color:#264d73;font-family:'Lato',sans-serif;"> <%= allergy[d] %></b>
              <h4  style="font-size:30px;font-weight: bold; color:#0a0a0a;font-family:'Lato',sans-serif;">ADVICE:<b style="font-size:25px;font-weight: bold; color:#264d73;font-family:'Lato',sans-serif;"> <%= advice[d] %></b>
              <h4  style="font-size:30px;font-weight: bold; color:#0a0a0a;font-family:'Lato',sans-serif;">SPECIAL CARE:<b style="font-size:25px;font-weight: bold; color:#264d73;font-family:'Lato',sans-serif;"> <%= splcare[d] %></b>
                <h4  style="font-size:30px;font-weight: bold; color:#0a0a0a;font-family:'Lato',sans-serif;">SUGGESTIONS:<b style="font-size:25px;font-weight: bold; color:#264d73;font-family:'Lato',sans-serif;"> <%= suggestion[d] %></b><br>
          
              
         
                        
                        
                            <button class="btn btn--radius-2 btn--blue" style="position: relative;left: 600px;"type="submit" value="<%= doc[d].data.pid%>" name="id" formaction="/docview">view</button>
                     
                      </form>
      </div>
      <% } %>

      
      
      </div>
            </div>
        </div>
    </div> 